    "class sgd_method:\n",
    "    \n",
    "    def __init__(self, model, learning_rate, weight_decay, batch_size):\n",
    "        self.model = model\n",
    "        self.lr = learning_rate\n",
    "        self.weight_decay = weight_decay\n",
    "        self.batch_size = batch_size\n",
    "\n",
    "    def zero_grad(self):\n",
    "        self.del_b = [np.zeros(bias.shape) for bias in self.model.biases]\n",
    "        self.del_w = [np.zeros(weight.shape) for weight in self.model.weights]\n",
    "\n",
    "    def update(self, delta_del_b, delta_del_w):\n",
    "        self.del_b = [nb + dnb for nb, dnb in zip(self.del_b, delta_del_b)]\n",
    "        self.del_w = [nw + dnw for nw, dnw in zip(self.del_w, delta_del_w)]\n",
    "\n",
    "    def step(self):\n",
    "        # batch gradient descent，将学习率除以batch_size\n",
    "        self.model.weights = [(1 - self.lr * self.weight_decay) * w - (self.lr / self.batch_size) * dw for w, dw in zip(self.model.weights, self.del_w)]\n",
    "        self.model.biases = [(1 - self.lr * self.weight_decay) * b - (self.lr / self.batch_size) * db for b, db in zip(self.model.biases, self.del_b)]"
   ]
